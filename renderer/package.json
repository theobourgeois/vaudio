{
  "name": "@vaudio/renderer",
  "version": "0.0.0",
  "description": "Renderer for vaudio",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build",
    "test": "jest"
  },
  "dependencies": {
    "@vaudio/core": "workspace:^",
    "ffmpeg-static": "^5.2.0",
    "fft.js": "^4.0.4",
    "fluent-ffmpeg": "^2.1.3",
    "gl": "^8.1.6"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/gl": "^6.0.5",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.5",
    "tsup": "^8.0.2",
    "typescript": "~5.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theobourgeois/vaudio.git"
  },
  "keywords": [
    "audio",
    "visualization"
  ],
  "author": "Th√©o Bourgeois",
  "license": "MIT"
}
